import("./include/stdlib.xs0");
var MAIN=class(){
    var func=function(var s1){
        //std.print(s);
        var arr=Array.init();
        std.print("Array Size:");
        var s=std.strToInt(std.input());
        for(var i=0,i<=s,i=i+1){
            std.print(i," ",s,":");
            var in=std.input();
            arr=Array.push(arr,in);
            delete in;
        };
        delete i;
        for(var i=0,Array.outsize(arr,i),i=i+1){
            std.print(Array.get(arr,i)," ");
        };
        delete i;
    };
};
MAIN.func(10);